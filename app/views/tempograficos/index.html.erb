<p id="notice"><%= notice %></p>

<h1>Listing Tempograficos</h1>

<table>
  <thead>
    <tr>
      <th>Tempo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tempograficos.each do |tempografico| %>
      <tr>
        <td><%= tempografico.tempo %></td>
        <td><%= link_to 'Show', tempografico %></td>
        <td><%= link_to 'Edit', edit_tempografico_path(tempografico) %></td>
        <td><%= link_to 'Destroy', tempografico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tempografico', new_tempografico_path %>
